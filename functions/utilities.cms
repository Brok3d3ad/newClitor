SUB(read_set,$path,$ini_variable,$variable)
    $value = INIREAD($path,$ini_variable)
    SETVAR($variable,$value)
END_SUB

SUB(read_set_cargoSlots,$ini_variable,$variable)
    $value = INIREAD($cargoSlotsPath,$ini_variable,$currentResolution)
    SETVAR($variable,$value)
END_SUB

